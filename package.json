{
    "name": "react-redux-boilerplate",
    "version": "1.0.0",
    "description": "React boilerplate",
    "scripts": {
        "clean": "rimraf dist",
        "lint": "eslint --ext jsx --ext js .",
        "lint:fix": "eslint --ext jsx --ext js --fix .",
        "webpack": "webpack --progress --display-error-details dummy.js",
        "build:webpack": "npm run lint && webpack --progress -p",
        "build:production": "npm run clean && cross-env NODE_ENV=production npm run build:webpack",
        "start": "node server.js"
    },
    "author": "cakraww",
    "dependencies": {
        "jquery": "^2.2.0",

        "react": "^0.14.7",
        "react-dom": "^0.14.7",

        "redux": "^3.3.1",
        "redux-thunk": "^1.0.3",
        "react-redux": "^4.4.0",

        "react-router": "^2.0.0",
        "react-router-redux": "^4.0.0",

        "react-helmet": "^2.3.1",
        "classnames": "^2.2.3",
        "isomorphic-fetch": "^2.2.1"
    },
    "devDependencies": {
        "express": "^4.4.5",
        "rimraf": "^2.5.2",
        "cross-env": "^1.0.7",

        "babel-core": "^6.5.2",
        "babel-loader": "^6.2.3",
        "babel-plugin-react-transform": "^2.0.0",
        "babel-polyfill": "^6.5.0",
        "babel-preset-es2015": "^6.5.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-react-hmre": "^1.1.0",

        "eslint": "^2.3.0",
        "eslint-import-resolver-webpack": "^0.1.5",
        "eslint-loader": "^1.3.0",
        "eslint-plugin-import": "^1.1.0",
        "eslint-plugin-react": "^4.2.0",

        "css-loader": "^0.23.1",
        "style-loader": "^0.13.0",

        "webpack": "^1.12.13",
        "webpack-dev-middleware": "^1.5.1",
        "webpack-hot-middleware": "^2.7.1"
    },
        "engines": {
        "node": "5.3.0"
    }
}
